{
  "name": "fhevm-example-template",
  "version": "1.0.0",
  "description": "Base template for FHEVM examples using Hardhat",
  "main": "index.ts",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat run scripts/deploy.ts --network sepolia",
    "clean": "hardhat clean",
    "typechain": "typechain --target ethers-v6 './artifacts/contracts/**/*.json'",
    "lint": "eslint --ext .ts contracts/ test/ scripts/"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "homomorphic-encryption",
    "privacy",
    "blockchain",
    "solidity",
    "hardhat"
  ],
  "author": "FHEVM Community",
  "license": "BSD-3-Clause-Clear",
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.3.0-1",
    "@fhevm/solidity": "^0.9.1",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v6": "^11.1.2",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "chai": "^4.3.7",
    "ethers": "^6.7.1",
    "hardhat": "^2.26.3",
    "hardhat-deploy": "^0.11.46",
    "typescript": "^5.9.2",
    "typechain": "^8.3.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
